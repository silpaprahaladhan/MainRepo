@model IEnumerable<Nirast.Pcms.Web.Models.UsersDetails>
<div class="x_panel">
    <div class="x_title">
        <h2>User List <small></small></h2>
        <ul class="nav navbar-right panel_toolbox">
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>

        </ul>
        <div class="clearfix"></div>
    </div>

    <div class="x_content">
        <div class="table-responsive">
            <input type="hidden" id="hdnCTId" />
            <table class="table table-striped jambo_table bulk_action" id="UserList">
                <thead>
                    <tr class="headings">

                        <th class="column-title col-md-1" style="display: table-cell;">User Name</th>
                        <th class="column-title" style="display: table-cell;">State</th>
                        <th class="column-title" style="display: table-cell;">City</th>
                        <th class="column-title" style="display: table-cell;">Email Address</th>
                        <th class="column-title" style="display: table-cell;">Phone Number</th>
                        <th>ViewDetails</th>

                    </tr>
                </thead>

                <tbody>
                    @if (Model != null)
                    {
                        foreach (var user in Model)
                        {
                            <tr class="even pointer">
                                <td class=" ">@user.FirstName @user.LastName</td>
                                <td class=" ">@user.State</td>
                                <td class=" ">@user.City</td>
                                <td class=" ">@user.EmailAddress</td>
                                <td class=" ">@user.PrimaryPhoneNo</td>
                                <td> @Html.ActionLink("View Profile", "UserProfile", "PublicUser", routeValues: new { id = user.UserRegnId }, htmlAttributes: new { @class = "viewDetails" })</td>
                            </tr>
                        }
                    }
                    </tbody>
            </table>
        </div>


    </div>
</div>
<script>
    $('#UserList').DataTable({
        "pagingType": "full_numbers"
    });
</script>
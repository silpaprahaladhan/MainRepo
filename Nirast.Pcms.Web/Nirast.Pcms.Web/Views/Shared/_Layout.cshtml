        <div class="col-sm-4 col-md-2 header-right footer-bottom">

            <div class="hidden navbar" id="loggedInUser">

                @*<button type="submit" formaction="/Home/Logout" id="btnLogout" class="theme-button hidden"><span>Logout</span></button>*@
                <div class="dropdown">

    <script type="text/javascript">
        $('ul.nav').find('a').click(function () {
            var $href = $(this).attr('href');
            var $anchor = $('#' + $href).offset();
            window.scrollTo($anchor.left, $anchor.top);
            return false;
        });

        $(document).ready(function () {
            $.ajax({
                url: '/Home/Footer',
                type: "GET",
                success: function (data) {

                    $("#footer").html(""); // clear before appending new list
                    $("#footer").html(data);
                }
            });
            $(".alert").fadeTo(10000, 500).slideUp(500, function () {
                $(".alert").slideUp(500);
            });

            if ($('#hdnLoggedUser').val() != '' && $('#hdnLoggedUser').val() != undefined && $('#hdnLoggedUser').val() != null) {

                $('.btnLogin').addClass("hidden");
                $('#loggedInUser').removeClass("hidden");
                if ($('#hdnProfilePic').val() != '' && $('#hdnProfilePic').val() != undefined && $('#hdnProfilePic').val() != null) {
                    $('#profilePic').attr('src', $('#hdnProfilePic').val());
                    $('#profilePic').removeClass('hidden');
                    $('#btnLogout').removeClass('hidden');
                    $('#lblUser').removeClass("hidden");
                    $('#lblUser').append($('#hdnLoggedUser').val());
                } else {
                    $('#profilePic').attr('src', '');
                    $('#profilePic').addClass('hidden');
                    $('#btnLogout').removeClass('hidden');
                    $('#lblUser').removeClass("hidden");
                    $('#lblUser').append($('#hdnLoggedUser').val());
                }
            } else {
                $('.btnLogin').removeClass("hidden");
                $('#loggedInUser').addClass("hidden");
                $('#btnLogout').addClass("hidden");
                $('#profilePic').addClass('hidden');
            }

            $('.image-toggler').click(function () {
                $('.image-toggle').hide();
                $($(this).attr('data-image-id')).show();
            });
            $('#signupuser').click(function () {
                window.location.href = "../PublicUser/UserRegistration";
            });
            $('#signupcaretaker').click(function () {
                window.location.href = "../CareTaker/CareTakerRegistration";
            });

            $('ul.nav').find('a').click(function () {
                var $href = $(this).attr('href');
                var $anchor = $('#' + $href).offset();
                window.scrollTo($anchor.left, $anchor.top);
                return false;
            });

            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 1000);
            });
        });

        jQuery(function ($) {
            var path = window.location.href; // because the 'href' property of the DOM element is the absolute path
            $('ul a').each(function () {
                $('ul a').removeClass('menu__item--current');
                if (this.URL === path) {

                    $(this).addClass('menu__item--current');
                }
            });
        });
    </script>


    @*<script src="~/js/Custom/ClientSearch.js"></script>*@
    <style type="text/css">
        @@-webkit-keyframes spin {
            0% {
                -webkit-transform: rotate(0deg);
            }

            100% {
                -webkit-transform: rotate(360deg);
            }
        }

        @@keyframes spin {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }

        .login-btn {
            background-color: #33a580;
            color: white;
            margin-top: -20px;
        }

        .content {
            width: 200px;
            height: 600px;
            background-color: blue;
            position: absolute; /*it can be fixed too*/
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
            margin: auto;
            /*this to solve "the content will not be cut when the window is smaller than the content": */
            max-width: 100%;
            max-height: 100%;

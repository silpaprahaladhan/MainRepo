                    <button class="dropbtn dropdown-toggle" data-toggle="dropdown">
                        <i class="fa fa-caret-down"></i>
                    </button>
                    <div class="dropdown-content dropdown-menu">
                        @if (Session["UserType"] != null)
                        {
                            if (Session["UserType"].ToString().ToUpper() == "PUBLIC USER")
                            {
                                <a id="btnLogin" href="@Url.Action("PublicUserDashboard", "PublicUser", new { id = Convert.ToInt32(Session["loggedInUserId"].ToString()) })">My Dashboard</a>
                                <a id="btnLogin" href="@Url.Action("UserProfile", "PublicUser", new { id = Convert.ToInt32(Session["loggedInUserId"].ToString()) })">View Profile</a>
                            }
                            else if (Session["UserType"].ToString().ToUpper() == "OFFICE STAFF" || Session["UserType"].ToString().ToUpper() == "ADMINISTRATOR")
                            {
                                <a id="btnLogin" href="@Url.Action("AdminDashboard", "Admin", null)">My Dashboard</a>
                                if (Session["UserType"].ToString().ToUpper() == "OFFICE STAFF")
                                {
                                    <a id="btnLogin" href="@Url.Action("ViewOfficeStaffProfile", "Admin", new { id = Convert.ToInt32(Session["loggedInUserId"].ToString()) })"> View Profile</a>
                                }
                                else if (Session["UserType"].ToString().ToUpper() == "ADMINISTRATOR")
                                {
                                    <a id="btnLogin" href="@Url.Action("AdminProfile", "Admin", new { id = Convert.ToInt32(Session["loggedInUserId"].ToString()) })"> View Profile</a>
                                }
                            }
                            else if (Session["UserType"].ToString().ToUpper() == "CARETAKER")
                            {
                                <a id="btnLogin" href="@Url.Action("CaretakerDashboardView", "Caretaker", new { id = Convert.ToInt32(Session["loggedInUserId"].ToString()) })">My Dashboard</a>
                                <a id="btnLogin" href="@Url.Action("CareTakerProfile", "Caretaker", new { id = Convert.ToInt32(Session["loggedInUserId"].ToString()) })">View Profile</a>
                            }
                        }


                        <a id="btnLogin" href="@Url.Action("ChangePassword", "Account")">Change Password</a>
                        @Html.ActionLink("Logout", "Logout", "Home", null, new { @class = " logout hidden", id = "btnLogout" })
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
    </div>
